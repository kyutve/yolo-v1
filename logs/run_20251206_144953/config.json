{
    "project": "YOLK",
    "seed": 42,
    "device": "cuda",
    "dataset": "bdd100k",
    "data_root": "./data",
    "classes": [
        "car",
        "traffic light",
        "person",
        "truck",
        "bus"
    ],
    "class_to_idx": {
        "car": 0,
        "traffic light": 1,
        "person": 2,
        "truck": 3,
        "bus": 4
    },
    "IMAGE_SIZE": 448,
    "batch_size": 32,
    "S": 7,
    "B": 2,
    "C": 5,
    "num_workers": 4,
    "augment": true,
    "_normal": {
        "mean": [
            0.485,
            0.456,
            0.406
        ],
        "std": [
            0.229,
            0.224,
            0.225
        ]
    },
    "backbone": "resnet50",
    "dropout_prob": 0.4,
    "l_coord": 7.0,
    "l_noobj": 0.6,
    "lr": 0.0003,
    "weight_decay": 0.0005,
    "optimizer": "adam",
    "epochs": 50,
    "lr_decay": true,
    "lr_gamma": 0.95,
    "save_dir": "./checkpoints",
    "log_dir": "./logs",
    "result_dir": "./results",
    "WARMUP_EPOCHS": 0,
    "EPSILON": 1e-06
}